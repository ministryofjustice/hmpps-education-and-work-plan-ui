{
  "request": {
    "method": "GET",
    "urlPathPattern": "/assets/.*/.*"
  },
  "response": {
    "headers": {
      "Content-Type": "text/css",
      "Cross-Origin-Resource-Policy": "same-site"
    },
    "status": 200,
    "bodyFileName": "assets/{{request.pathSegments.[1]}}/{{request.pathSegments.[2]}}"
  }
}
